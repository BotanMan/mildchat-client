{
  "name": "mildchat-client",
  "description": "Mildchat Cliet for Angular2 workshops",
  "license": "MIT",
  "version": "1.0.0",
  "repository": "https://github.com/lwojciechowski/mildchat-client",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
